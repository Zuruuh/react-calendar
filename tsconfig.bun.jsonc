{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "paths": {
      "@zuruuh/react-calendar": ["./src/"]
    }
  },
  "include": [
    "build.ts",
    "bootstrap.tsx",
    ".ladle/vite.config.ts",
    "package.json",
    "docs/.vitepress/config.ts"
  ]
}
